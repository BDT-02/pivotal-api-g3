{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "created at",
    "updated at",
    "name",
    "plan",
    "status"
   ],
   "properties": {
     "created at": {
       "$id": "#/properties/created at",
       "type": "string",
       "title": "The Created at Schema",
       "default": "",
       "examples": [
         "2019-01-07T21:42:45Z"
       ],
       "pattern": "^(.*)$"
     },
     "updated at": {
       "$id": "#/properties/updated at",
       "type": "string",
       "title": "The Updated at Schema",
       "default": "",
       "examples": [
         "2019-01-07T21:42:45Z"
       ],
       "pattern": "^(.*)$"
     },
     "name": {
       "$id": "#/properties/name",
       "type": "string",
       "title": "The Name Schema",
       "default": "",
       "examples": [
         "A green door"
       ],
       "pattern": "^(.*)$"
     },
     "plan": {
       "$id": "#/properties/plan",
       "type": "string",
       "title": "The Plan Schema",
       "default": "",
       "examples": [
         "Test"
       ],
       "pattern": "^(.*)$"
     },
     "status": {
       "$id": "#/properties/status",
       "type": "integer",
       "title": "The Status Schema",
       "default": 0,
       "examples": [
         200
       ]
     }
   }
 }
